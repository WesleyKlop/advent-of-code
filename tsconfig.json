{
    "compilerOptions": {
        "module": "esnext",
        "esModuleInterop": true,
        "strict": false,
        "target": "ES5",
        "noImplicitAny": false,
        "moduleResolution": "node",
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "*": ["node_modules/*", "src/*"]
        }
    },
    "include": ["src/**/*"]
}
