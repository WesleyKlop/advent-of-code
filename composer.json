{
    "name": "wesleyklop/advent-of-code",
    "description": "Advent of code solutions in PHP",
    "keywords": [
        "framework",
        "laravel",
        "laravel zero",
        "console",
        "cli"
    ],
    "homepage": "https://wesley.io",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Wesley Klop",
            "email": "wesley19097@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^7.2.0",
        "illuminate/http": "^8.0",
        "laravel-zero/framework": "^8.0"
    },
    "require-dev": {
        "intonate/tinker-zero": "^1.1",
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^0.3",
        "symplify/easy-coding-standard": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "advent-of-code"
    ],
    "scripts": {
        "lint:fix": "ecs check app bootstrap config tests --fix",
        "lint:check": "ecs check app bootstrap config tests"
    }
}
